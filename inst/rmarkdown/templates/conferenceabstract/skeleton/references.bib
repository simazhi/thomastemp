@article{Eriksen2010,
  title = {Linguistics of Weather: Cross-Linguistic Patterns of Meteorological Expressions},
  volume = {34},
  abstract = {his paper is a cross-linguistic investigation of meteorological expressions (such as it is snowing or the wind blows). The paper proposes a typology of meteorological constructions that are divided into three according to the element primarily responsible for the coding of weather. In the predicate type, a predicate expresses the meteorological event, while an argument has other functions. In the argument type, an argument is responsible for expressing weather, while any eventual predicate is semantically rather vacuous. In the argument-predicate type, finally, a predicate and an argument are both involved. All types include subtypes, depending on the syntactic valency and the parts of speech of the elements involved. Building upon the typology of constructions, a typology of languages is also proposed based on the coding of precipitation and temperature.
Keywords: expletive subjects, argument structure, meteorological expressions, syntax, typology},
  number = {3},
  journaltitle = {Studies in Language},
  date = {2010},
  pages = {565--601},
